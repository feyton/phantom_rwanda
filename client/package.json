{
  "name": "phantom-fe-codebandits",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "webpack serve",
    "build": "cross-env NODE_ENV=production  webpack",
    "prepare": "husky install",
    "format": "npx pretty-quick",
    "lint": "npx eslint src/*.j{s,sx} --fix",
    "test": "jest --watch",
    "coverage": "jest --coverage --silent",
    "testPro": "npm run build && cd dist && http-server",
    "dev": "npx nodemon src/server.js"
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/tests/setupTests.js"
    ],
    "moduleNameMapper": {
      "\\.(css|scss)$": "identity-obj-proxy",
      "^@/(.*)$": "<rootDir>/src/$1",
      "^@pages/(.*)$": "<rootDir>/src/pages/$1",
      "^@components/(.*)$": "<rootDir>/src/components/$1",
      "^@reducers/(.*)$": "<rootDir>/src/redux/reducers/$1",
      "^@forms/(.*)$": "<rootDir>/src/forms/$1",
      "^@utils/(.*)$": "<rootDir>/src/utils/$1",
      "^react(.*)$": "<rootDir>/node_modules/react$1"
    },
    "transform": {
      "\\.(js|jsx)$": "babel-jest",
      "\\.(jpg|jpeg|png|svg)$": "<rootDir>/config/jest/fileMock.js"
    },
    "coverageReporters": [
      "json",
      "lcov",
      "text"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/src/index.js",
      "<rootDir>/src/server.js",
      "<rootDir>/src/utils/Api.js"
    ],
    "collectCoverage": true,
    "testEnvironment": "jsdom"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/atlp-rwanda/phantom-fe-codebandits.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/atlp-rwanda/phantom-fe-codebandits/issues"
  },
  "dependencies": {
    "@babel/plugin-transform-runtime": "^7.17.0",
